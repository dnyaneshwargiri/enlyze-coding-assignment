{
  "name": "enlyze-kpi-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "kpi-builder",
    "kpi-svc"
  ],
  "scripts": {
    "dev": "yarn workspace kpi-builder vite",
    "build": "yarn workspace kpi-builder tsc -b && vite build",
    "svc:start": "yarn workspace kpi-svc build && yarn workspace kpi-svc start",
    "svc:dev": "yarn workspace kpi-svc dev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest"
  }
}
